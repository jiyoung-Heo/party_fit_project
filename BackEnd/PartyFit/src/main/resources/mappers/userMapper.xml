<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace= "com.ssafy.partyfit.model.dao.UserDao">
  
  
  <insert id="insertUser" parameterType="User" >
  
  INSERT INTO user(login_id,name,password,email,username) VALUES ( #{loginId},#{name},#{password},#{email},#{username})
  </insert>
  
    <select id ="selectByInfo" parameterType="User" resultType="User">
  
		SELECT *
		FROM user
		WHERE login_id = #{loginId} AND password = #{password}
  
  </select>

  </mapper>